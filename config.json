          "style": {
            "backgroundColor": "#0000FF"
          }
        }
      ]
    },
    {
      "name": "CSVファイル2",
      "dir": "./test-csv/data2"
    },
    {
      "name": "CSVファイル",
      "dir": "./test-csv",
      "form": [
        {
          "expression": "A0 == 'NG'",
          "style": {
            "color": "red",
            "fontWeight": "bold"
          }
        },
        {
          "range": "B2",
          "expression": "$B0 != ''",
          "style": {
            "backgroundColor": "#FFE0E0"
          }
        }
      ]
    },
    {
      "name": "ENVファイル",
